# Database Configuration
# Set to "postgresql" to use PostgreSQL, or "sqlite" for SQLite
DB_TYPE=postgresql

# PostgreSQL Database Configuration
# Copy this file to config.env and update the values according to your PostgreSQL setup

DB_USER=postgres
DB_PASSWORD=postgres
DB_HOST=localhost
DB_PORT=5432
DB_NAME=cafe_users

# Twilio SMS Configuration
# Get these values from your Twilio Console (https://console.twilio.com/)
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=+1234567890

# Optional: Set to True to enable debug logging
DEBUG=True

# Super Admin Configuration
# Only this email will be allowed to register as super admin
SUPER_ADMIN_EMAIL=superadmin@yourdomain.com 